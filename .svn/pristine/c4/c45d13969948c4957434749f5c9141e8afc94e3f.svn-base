<title>ccc</title>
</head>
<body style="background: #fff;">
	<div class="container no-padding">
		<div class="row">
			<div class="col-md-12">
			<input type="text" name="val" id = "val"/> 
			<button id = "sub">预测</button>
			</div>
		</div>
	</div>
	<script src="js/jquery-3.1.0.min.js"></script>
	<script>
		$("#sub").on("click",function(){
			var val = $("#val").val();
			window.location.href = "validate?val="+val;	
		});
	</script>
</body>
